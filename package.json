{
    "name": "angular-starter",
    "version": "1.0.0",
    "description": "Angular webpack starter",
    "author": "Parallel Task Authors",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/ParallelTask/angular-starter",
        "type": "git"
    },
    "private": true,
    "scripts": {
        "clean": "rimraf dist",
        "clean:prod": "rimraf public",
        "build": "npm run clean && webpack --config config/webpack.dev.js --progress --bail",
        "build:watch": "npm run clean && webpack --config config/webpack.dev.js --watch --inline --progress --bail",
        "publish": "npm run clean:prod && webpack --config config/webpack.prod.js --progress --bail",
        "serve": "webpack-dev-server --config config/webpack.dev.js --port 8080 --hot --open --progress --bail",
        "serve:lazy": "webpack-dev-server --compress --config config/webpack.dev.js --lazy --port 8081 --open --inline --progress --bail",
        "serve:dev": "light-server -s dist -p 8082 -o --historyindex ./index.html",
        "serve:prod": "http-server public -p 8088 --push-state -o -c10",
        "lint": "tslint -p tsconfig.json -c tslint.json './src/**/*.ts*'",
        "start": "npm run publish && npm run serve:prod",
        "generate": "./node_modules/.bin/compodoc -p tsconfig.json"
    },
    "dependencies": {
        "@angular/animations": "^5.1.2",
        "@angular/common": "^5.1.2",
        "@angular/compiler": "^5.1.2",
        "@angular/core": "^5.1.2",
        "@angular/forms": "^5.1.2",
        "@angular/http": "^5.1.2",
        "@angular/platform-browser": "^5.1.2",
        "@angular/platform-browser-dynamic": "^5.1.2",
        "@angular/router": "^5.1.2",
        "bootstrap": "^3.3.7",
        "core-js": "^2.4.1",
        "immutable": "^3.8.2",
        "lodash": "^4.17.4",
        "moment": "^2.20.1",
        "primeng": "^5.0.2",
        "rxjs": "^5.5.1",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@compodoc/compodoc": "^1.0.5",
        "@types/lodash": "^4.14.91",
        "@types/node": "^7.0.22",
        "angular-router-loader": "^0.8.1",
        "angular2-template-loader": "^0.6.2",
        "awesome-typescript-loader": "^3.1.2",
        "clean-webpack-plugin": "^0.1.17",
        "codelyzer": "^4.0.2",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.26.1",
        "extract-text-webpack-plugin": "^2.1.2",
        "file-loader": "^1.1.6",
        "html-loader": "^0.4.5",
        "html-webpack-externals-plugin": "^3.6.0",
        "html-webpack-include-assets-plugin": "^1.0.2",
        "html-webpack-plugin": "^2.28.0",
        "light-server": "^2.2.1",
        "node-sass": "^4.1.1",
        "npm-run-all": "^4.1.2",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.2",
        "sass-loader": "^6.0.6",
        "spa-http-server": "^0.9.0",
        "style-loader": "^0.13.1",
        "to-string-loader": "^1.1.5",
        "tslint": "^5.8.0",
        "typescript": "~2.4.2",
        "uglifyjs-webpack-plugin": "^1.1.5",
        "url-loader": "^0.6.2",
        "webpack": "^2.3.1",
        "webpack-conditional-loader": "^1.0.11",
        "webpack-dev-server": "~2.5.0",
        "webpack-merge": "^4.1.1"
    },
    "engines": {
        "node": ">= 6.9.x",
        "npm": ">= 3.10.x"
    }
}